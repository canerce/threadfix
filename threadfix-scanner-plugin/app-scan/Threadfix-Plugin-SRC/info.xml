<AppScanExtension>
  <!-- Front End MetaData -->
  <FullName>Threadfix Plugin</FullName>
  <Description>
    This eXtension enables AppScan users to pre-seed appscan using data from Threadfix.
  </Description>
  <Version>1.0</Version>
  <Author>Denim Group</Author>
  <MainDllFile>Threadfix-Plugin.dll</MainDllFile>
  <Copyright>Denim Group, Ltd. 2009-2015. All Rights Reserved</Copyright>
  <HomepageURL>http://www.threadfix.org/</HomepageURL>

  <!-- Target Application this extension can install into, 
       with minimum and maximum supported versions. -->
  <TargetEdition>

    <!-- supported values: AppScanAuditors, AppScanDev, AppScanQA, AppScanXM, All -->
    <ID>All</ID>

    <MinCompatibleVersion>8.6</MinCompatibleVersion>

  </TargetEdition>
</AppScanExtension>
